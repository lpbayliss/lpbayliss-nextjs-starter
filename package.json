{
  "name": "lpbayliss-nextjs-starter",
  "version": "1.0.0",
  "description": "A React starter kit using next.js, typescript, emotion, tailwindcss, jest + enzyme, storybook and prettier.",
  "repository": "https://github.com/lpbayliss/nextjs-starter/blob/master/package.json",
  "main": "index.js",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "type-check": "tsc --pretty --noEmit",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "format": "prettier --write \"**/*.{js,jsx,json,ts,tsx,md,mdx,css,html,yml,yaml}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type-check"
    }
  },
  "lint-staged": {
    "*.@(js|jsx|ts|tsx)": [
      "npm run lint",
      "npm run format",
      "git add"
    ]
  },
  "keywords": [
    "react",
    "javascript",
    "typescript",
    "emotion",
    "Storybook",
    "nextjs",
    "chakra",
    "jest",
    "starter",
    "kickstart      "
  ],
  "author": "Luke Bayliss",
  "license": "ISC",
  "dependencies": {
    "@chakra-ui/core": "^0.8.0",
    "@emotion/core": "^10.0.35",
    "@emotion/styled": "^10.0.27",
    "emotion": "^10.0.27",
    "emotion-normalize": "^10.1.0",
    "emotion-server": "^10.0.27",
    "emotion-theming": "^10.0.27",
    "next": "^11.1.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@storybook/addon-actions": "^6.0.5",
    "@storybook/addon-essentials": "^6.0.5",
    "@storybook/addon-knobs": "^6.0.5",
    "@storybook/addon-links": "^6.0.5",
    "@storybook/addons": "^6.0.5",
    "@storybook/react": "^6.0.5",
    "@testing-library/jest-dom": "^5.11.2",
    "@testing-library/react": "^10.4.7",
    "@types/jest": "^26.0.8",
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/testing-library__react": "^10.2.0",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "babel-jest": "^26.2.2",
    "babel-loader": "^8.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-react": "^7.20.5",
    "husky": "^4.2.5",
    "jest": "^26.2.2",
    "jest-watch-typeahead": "^0.6.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "react-is": "^16.13.1",
    "ts-loader": "^8.0.2",
    "ts-node": "^8.10.2",
    "typescript": "3.8.3"
  }
}
